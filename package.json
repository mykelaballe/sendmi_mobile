{
  "name": "sendmi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm i",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html",
    "tron": "node_modules/.bin/reactotron",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82"
  },
  "dependencies": {
    "ImageViewer": "^1.5.7",
    "apisauce": "^0.5.0",
    "format-json": "^1.0.3",
    "moment": "^2.29.2",
    "native-base": "^0.5.7",
    "querystringify": "0.0.4",
    "ramda": "^0.22.1",
    "react": "15.3.2",
    "react-native": "^0.34.0",
    "react-native-action-button": "^2.0.1",
    "react-native-animatable": "^0.6.0",
    "react-native-app-intro": "^1.1.4",
    "react-native-background-timer": "^0.1.3",
    "react-native-camera": "^0.4.1",
    "react-native-code-push": "^1.16.1-beta",
    "react-native-collapsible": "^0.7.0",
    "react-native-communications": "^2.2.1",
    "react-native-config": "github:skellock/react-native-config#fix-missing-env",
    "react-native-device-info": "^0.9.7",
    "react-native-drawer": "^2.3.0",
    "react-native-drawer-layout": "^1.1.0",
    "react-native-fbsdk": "^0.6.1",
    "react-native-fcm": "^2.5.6",
    "react-native-fetch-blob": "^0.10.4",
    "react-native-file-picker": "0.0.5",
    "react-native-i18n": "0.0.8",
    "react-native-image-crop-picker": "^0.11.2",
    "react-native-image-fit": "^0.9.10",
    "react-native-image-picker": "^0.24.1",
    "react-native-image-progress": "^0.6.0",
    "react-native-image-resizer": "^0.1.0",
    "react-native-image-viewer": "0.0.3",
    "react-native-keep-awake": "^1.0.7",
    "react-native-keyboard-aware-scroll-view": "^0.2.9",
    "react-native-keychain": "^0.3.2",
    "react-native-loading-spinner-overlay": "^0.3.0",
    "react-native-modal-picker": "0.0.16",
    "react-native-parallax-scroll-view": "^0.19.0",
    "react-native-photo-browser": "^0.2.2",
    "react-native-picker": "^4.0.10",
    "react-native-popup-menu": "^0.7.3",
    "react-native-progress": "^3.0.1",
    "react-native-root-toast": "^1.0.5",
    "react-native-router-flux": "^3.35.0",
    "react-native-scrollable-tab-view": "^0.6.0",
    "react-native-sensor-manager": "^0.1.10",
    "react-native-smart-badge": "^1.1.1",
    "react-native-sound": "^0.8.3",
    "react-native-storage": "^0.1.4",
    "react-native-table-component": "^1.1.7",
    "react-native-timer-mixin": "^0.11.0",
    "react-native-vector-icons": "^2.1.0",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-logger": "^2.4.0",
    "redux-persist": "^3.5.0",
    "redux-saga": "^0.11.1",
    "reduxsauce": "0.1.0",
    "rn-splash-screen": "^2.1.0",
    "seamless-immutable": "^6.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.14.0",
    "enzyme": "^2.3.0",
    "ghooks": "^1.3.2",
    "mockery": "^1.7.0",
    "nyc": "^8.1.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-native-mock": "^0.2.6",
    "reactotron-apisauce": "^1.1.2",
    "reactotron-react-native": "^1.1.2",
    "reactotron-redux": "^1.1.2",
    "snazzy": "^4.0.1",
    "standard": "^8.0.0"
  },
  "ava": {
    "files": [
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "if [ -d 'ignite-base' ]; then cd ignite-base; fi; npm run lint"
    }
  }
}
